{
  "extends": "airbnb",
  "rules": {
    "func-names": ["error", "never"],
    "jsx-a11y/label-has-for": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": 0,
    "class-methods-use-this": 0,
    "no-alert": 0,
    "no-nested-ternary": 0,
    "no-lonely-if": 0,
    "no-return-assign": 0,
    "prefer-promise-reject-errors": 0,
    "import/no-extraneous-dependencies": 0,
    "no-plusplus": 0,
  }
}
